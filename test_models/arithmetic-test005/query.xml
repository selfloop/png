<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>arithmetic-test005.FALSE-1</id>
    <description>arithmetic-test005.FALSE-1</description>
    <formula>
      <exists-path>
        <finally>
          <integer-eq>
            <integer-difference>
              <tokens-count>
                <place>P0</place>
              </tokens-count>
              <tokens-count>
                <place>P1</place>
              </tokens-count>
              <tokens-count>
                <place>P2</place>
              </tokens-count>
            </integer-difference>
            <integer-constant>0</integer-constant>
          </integer-eq>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>arithmetic-test005.TRUE-2</id>
    <description>arithmetic-test005.TRUE-2</description>
    <formula>
      <exists-path>
        <finally>
          <integer-eq>
            <integer-difference>
              <tokens-count>
                <place>P0</place>
              </tokens-count>
              <tokens-count>
                <place>P1</place>
              </tokens-count>
              <tokens-count>
                <place>P2</place>
              </tokens-count>
            </integer-difference>
            <integer-constant>-1</integer-constant>
          </integer-eq>
        </finally>
      </exists-path>
    </formula>
  </property>
</property-set>
