<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>reduction-024.TRUE-1</id>
    <description>reduction-024.TRUE-1</description>
    <formula>
      <exists-path>
        <finally>
          <integer-ge>
            <tokens-count>
              <place>Shared_NotPrime</place>
            </tokens-count>
            <integer-constant>1</integer-constant>
          </integer-ge>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>reduction-024.TRUE-2</id>
    <description>reduction-024.TRUE-2</description>
    <formula>
      <all-paths>
        <globally>
          <disjunction>
            <integer-le>
              <tokens-count>
                <place>MoveDualtoNumber_P4</place>
              </tokens-count>
              <integer-constant>2</integer-constant>
            </integer-le>
            <integer-le>
              <tokens-count>
                <place>Shared_IsPrime</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-le>
          </disjunction>
        </globally>
      </all-paths>
