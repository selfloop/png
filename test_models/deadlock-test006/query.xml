<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>deadlock-test006.TRUE-1</id>
    <description>deadlock-test006.TRUE-1</description>
    <formula>
      <all-paths>
        <finally>
          <disjunction>
            <conjunction>
              <integer-eq>
                <tokens-count>
                  <place>WorkflowNet1_out</place>
                </tokens-count>
                <integer-constant>1</integer-constant>
              </integer-eq>
              <integer-eq>
                <tokens-count>
                  <place>WorkflowNet2_out</place>
                </tokens-count>
                <integer-constant>1</integer-constant>
              </integer-eq>
            </conjunction>
            <integer-eq>
              <tokens-count>
                <place>WorkflowNet2_DelayStart</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-eq>
          </disjunction>
        </finally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>deadlock-test006.FALSE-2</id>
    <description>deadlock-test006.FALSE-2</description>
    <formula>
      <all-paths>
        <finally>
          <conjunction>
            <integer-eq>
              <tokens-count>
                <place>WorkflowNet1_out</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>WorkflowNet2_out</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-eq>
          </conjunction>
        </finally>
      </all-paths>
    </formula>
  </property>
</property-set>
