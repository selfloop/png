<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>weighted-test003.FALSE-1</id>
    <description>weighted-test003.FALSE-1</description>
    <formula>
      <exists-path>
        <finally>
          <integer-ge>
            <tokens-count>
              <place>FinalNode</place>
            </tokens-count>
            <integer-constant>1</integer-constant>
          </integer-ge>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>weighted-test003.TRUE-2</id>
    <description>weighted-test003.TRUE-2</description>
    <formula>
      <exists-path>
        <globally>
          <disjunction>
            <integer-ge>
              <tokens-count>
                <place>P2</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-ge>
            <integer-ge>
              <tokens-count>
                <place>P1</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-ge>
            <integer-ge>
              <tokens-count>
                <place>StartNode</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-ge>
          </disjunction>
        </globally>
      </exists-path>
    </formula>
  </property>
</property-set>
