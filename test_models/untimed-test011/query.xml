<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>untimed-test011.FALSE-1</id>
    <description>untimed-test011.FALSE-1</description>
    <formula>
      <all-paths>
        <globally>
          <integer-eq>
            <tokens-count>
              <place>P2</place>
            </tokens-count>
            <tokens-count>
              <place>P3</place>
            </tokens-count>
          </integer-eq>
        </globally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>untimed-test011.TRUE-2</id>
    <description>untimed-test011.TRUE-2</description>
    <formula>
      <exists-path>
        <finally>
          <integer-ne>
            <tokens-count>
              <place>P3</place>
            </tokens-count>
            <tokens-count>
              <place>P2</place>
            </tokens-count>
          </integer-ne>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>untimed-test011.FALSE-3</id>
    <description>untimed-test011.FALSE-3</description>
    <formula>
      <all-paths>
        <globally>
          <integer-ne>
            <tokens-count>
              <place>P2</place>
            </tokens-count>
            <tokens-count>
              <place>P4</place>
            </tokens-count>
          </integer-ne>
        </globally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>untimed-test011.TRUE-4</id>
    <description>untimed-test011.TRUE-4</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-eq>
              <tokens-count>
                <place>P2</place>
              </tokens-count>
              <tokens-count>
                <place>P5</place>
              </tokens-count>
            </integer-eq>
            <integer-gt>
              <tokens-count>
                <place>P2</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-gt>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
</property-set>
