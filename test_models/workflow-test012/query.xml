<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>workflow-test012.TRUE-1</id>
    <description>workflow-test012.TRUE-1</description>
    <formula>
      <exists-path>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>Out</place>
            </tokens-count>
            <integer-constant>2</integer-constant>
          </integer-eq>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>workflow-test012.FALSE-2</id>
    <description>workflow-test012.FALSE-2</description>
    <formula>
      <exists-path>
        <finally>
          <integer-ge>
            <tokens-count>
              <place>Out</place>
            </tokens-count>
            <integer-constant>3</integer-constant>
          </integer-ge>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>workflow-test012.FALSE-3</id>
    <description>workflow-test012.FALSE-3</description>
    <formula>
      <all-paths>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>Out</place>
            </tokens-count>
            <integer-constant>1</integer-constant>
          </integer-eq>
        </finally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>workflow-test012.TRUE-4</id>
    <description>workflow-test012.TRUE-4</description>
    <formula>
      <exists-path>
        <globally>
          <disjunction>
            <integer-ge>
              <tokens-count>
                <place>WR</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-ge>
            <integer-ge>
              <tokens-count>
                <place>WRoom</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-ge>
          </disjunction>
        </globally>
      </exists-path>
    </formula>
  </property>
</property-set>
