<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>arithmetic-test001.TRUE-1</id>
    <description>arithmetic-test001.TRUE-1</description>
    <formula>
      <all-paths>
        <globally>
          <integer-le>
            <integer-sum>
              <tokens-count>
                <place>FORK_1</place>
              </tokens-count>
              <tokens-count>
                <place>FORK_2</place>
              </tokens-count>
              <tokens-count>
                <place>FORK_3</place>
              </tokens-count>
              <tokens-count>
                <place>FORK_4</place>
              </tokens-count>
              <tokens-count>
                <place>FORK_5</place>
              </tokens-count>
              <tokens-count>
                <place>FORK_6</place>
              </tokens-count>
            </integer-sum>
            <integer-constant>6</integer-constant>
          </integer-le>
        </globally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>arithmetic-test001.TRUE-2</id>
    <description>arithmetic-test001.TRUE-2</description>
    <formula>
      <exists-path>
        <globally>
          <integer-lt>
            <integer-sum>
              <tokens-count>
                <place>EAT_1</place>
              </tokens-count>
              <tokens-count>
                <place>EAT_2</place>
              </tokens-count>
              <tokens-count>
                <place>EAT_3</place>
              </tokens-count>
            </integer-sum>
            <integer-sum>
              <tokens-count>
                <place>EAT_4</place>
              </tokens-count>
              <tokens-count>
                <place>EAT_5</place>
              </tokens-count>
              <integer-constant>2</integer-constant>
            </integer-sum>
          </integer-lt>
        </globally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>arithmetic-test001.FALSE-3</id>
    <description>arithmetic-test001.FALSE-3</description>
    <formula>
      <exists-path>
        <finally>
          <integer-lt>
            <integer-product>
              <integer-constant>5</integer-constant>
              <integer-constant>8</integer-constant>
            </integer-product>
            <integer-constant>40</integer-constant>
          </integer-lt>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>arithmetic-test001.TRUE-4</id>
    <description>arithmetic-test001.TRUE-4</description>
    <formula>
      <all-paths>
        <globally>
          <integer-lt>
            <integer-difference>
              <integer-constant>6</integer-constant>
              <integer-product>
                <integer-constant>5</integer-constant>
                <integer-constant>8</integer-constant>
              </integer-product>
            </integer-difference>
            <integer-constant>9</integer-constant>
          </integer-lt>
        </globally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>arithmetic-test001.FALSE-5</id>
    <description>arithmetic-test001.FALSE-5</description>
    <formula>
      <exists-path>
        <finally>
          <integer-eq>
            <integer-product>
              <integer-constant>2</integer-constant>
              <tokens-count>
                <place>FORK_1</place>
              </tokens-count>
            </integer-product>
            <integer-sum>
              <integer-constant>1</integer-constant>
              <tokens-count>
                <place>FORK_2</place>
              </tokens-count>
              <integer-difference>
                <tokens-count>
                  <place>FORK_3</place>
                </tokens-count>
                <integer-constant>7</integer-constant>
              </integer-difference>
            </integer-sum>
          </integer-eq>
        </finally>
      </exists-path>
    </formula>
  </property>
</property-set>
