  <property>
    <id>composition-test009.FALSE-1</id>
    <description>composition-test009.FALSE-1</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-ge>
              <tokens-count>
                <place>Train_Crossing</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-ge>
            <integer-ge>
              <tokens-count>
                <place>TragicLight_Green</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-ge>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>composition-test009.TRUE-2</id>
    <description>composition-test009.TRUE-2</description>
    <formula>
      <exists-path>
        <finally>
          <deadlock/>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>composition-test009.FALSE-3</id>
    <description>composition-test009.FALSE-3</description>
    <formula>
      <all-paths>
        <globally>
          <negation>
            <deadlock/>
          </negation>
        </globally>
      </all-paths>
    </formula>
  </property>
