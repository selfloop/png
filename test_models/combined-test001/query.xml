<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>combined-test001.TRUE-1</id>
    <description>combined-test001.TRUE-1</description>
    <formula>
      <exists-path>
        <finally>
          <integer-ge>
            <tokens-count>
              <place>Final</place>
            </tokens-count>
            <integer-constant>4</integer-constant>
          </integer-ge>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>combined-test001.TRUE-2</id>
    <description>combined-test001.TRUE-2</description>
    <formula>
      <all-paths>
        <globally>
          <disjunction>
            <integer-ge>
              <tokens-count>
                <place>P0</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-ge>
            <integer-ge>
              <tokens-count>
                <place>P4</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-ge>
          </disjunction>
        </globally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>combined-test001.TRUE-3</id>
    <description>combined-test001.TRUE-3</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-ge>
              <tokens-count>
                <place>Final</place>
              </tokens-count>
              <integer-constant>4</integer-constant>
            </integer-ge>
            <integer-le>
              <tokens-count>
                <place>P2</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-le>
            <integer-eq>
              <tokens-count>
                <place>P1</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
            <disjunction>
              <integer-eq>
                <tokens-count>
                  <place>P0</place>
                </tokens-count>
                <integer-constant>0</integer-constant>
              </integer-eq>
              <integer-eq>
                <tokens-count>
                  <place>P4</place>
                </tokens-count>
                <integer-constant>0</integer-constant>
              </integer-eq>
            </disjunction>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>combined-test001.FALSE-4</id>
    <description>combined-test001.FALSE-4</description>
    <formula>
      <exists-path>
        <finally>
          <integer-ge>
            <tokens-count>
              <place>Final</place>
            </tokens-count>
            <integer-constant>4</integer-constant>
          </integer-ge>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>combined-test001.FALSE-5</id>
    <description>combined-test001.FALSE-5</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-eq>
              <tokens-count>
                <place>P1</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>P2</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>P0</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
</property-set>
