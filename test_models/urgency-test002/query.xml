<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>urgency-test002.TRUE-1</id>
    <description>urgency-test002.TRUE-1</description>
    <formula>
      <exists-path>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>P8</place>
            </tokens-count>
            <integer-constant>1</integer-constant>
          </integer-eq>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>urgency-test002.FALSE-2</id>
    <description>urgency-test002.FALSE-2</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-eq>
              <tokens-count>
                <place>P7</place>
              </tokens-count>
              <integer-constant>5</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>P8</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-eq>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>urgency-test002.TRUE-3</id>
    <description>urgency-test002.TRUE-3</description>
    <formula>
      <all-paths>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>P8</place>
            </tokens-count>
            <integer-constant>1</integer-constant>
          </integer-eq>
        </finally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>urgency-test002.FALSE-4</id>
    <description>urgency-test002.FALSE-4</description>
    <formula>
      <exists-path>
        <globally>
          <integer-ge>
            <tokens-count>
              <place>P7</place>
            </tokens-count>
            <integer-constant>4</integer-constant>
          </integer-ge>
        </globally>
      </exists-path>
    </formula>
  </property>
</property-set>
