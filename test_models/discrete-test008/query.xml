<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>discrete-test008.FALSE-1</id>
    <description>discrete-test008.FALSE-1</description>
    <formula>
      <all-paths>
        <finally>
          <integer-ge>
            <tokens-count>
              <place>P3</place>
            </tokens-count>
            <integer-constant>3</integer-constant>
          </integer-ge>
        </finally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>discrete-test008.FALSE-2</id>
    <description>discrete-test008.FALSE-2</description>
    <formula>
      <exists-path>
        <globally>
          <integer-lt>
            <tokens-count>
              <place>P2</place>
            </tokens-count>
            <integer-constant>0</integer-constant>
          </integer-lt>
        </globally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>discrete-test008.TRUE-3</id>
    <description>discrete-test008.TRUE-3</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-eq>
              <tokens-count>
                <place>P1</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>P3</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-eq>
            <negation>
              <integer-eq>
                <tokens-count>
                  <place>P0</place>
                </tokens-count>
                <integer-constant>1</integer-constant>
              </integer-eq>
            </negation>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>discrete-test008.TRUE-4</id>
    <description>discrete-test008.TRUE-4</description>
    <formula>
      <exists-path>
        <globally>
          <disjunction>
            <negation>
              <integer-eq>
                <tokens-count>
                  <place>P3</place>
                </tokens-count>
                <integer-constant>1</integer-constant>
              </integer-eq>
            </negation>
            <integer-eq>
              <tokens-count>
                <place>P2</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
          </disjunction>
        </globally>
      </exists-path>
    </formula>
  </property>
</property-set>
