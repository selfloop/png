<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>exercise-test001.TRUE-1</id>
    <description>exercise-test001.TRUE-1</description>
    <formula>
      <exists-path>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>P19</place>
            </tokens-count>
            <integer-constant>1</integer-constant>
          </integer-eq>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>exercise-test001.TRUE-2</id>
    <description>exercise-test001.TRUE-2</description>
    <formula>
      <all-paths>
        <globally>
          <disjunction>
            <integer-eq>
              <tokens-count>
                <place>P17</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>P16</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>P18</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>P19</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
          </disjunction>
        </globally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>exercise-test001.FALSE-3</id>
    <description>exercise-test001.FALSE-3</description>
    <formula>
      <exists-path>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>P18</place>
            </tokens-count>
            <integer-constant>2</integer-constant>
          </integer-eq>
        </finally>
      </exists-path>
    </formula>
  </property>
</property-set>
