                  <place>P3</place>
                </tokens-count>
                <integer-constant>0</integer-constant>
              </integer-le>
              <integer-ge>
                <tokens-count>
                  <place>P4</place>
                </tokens-count>
                <integer-constant>1</integer-constant>
              </integer-ge>
