<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>query-test001.FALSE-1</id>
    <description>query-test001.FALSE-1</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-ge>
              <tokens-count>
                <place>P1</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-ge>
            <integer-ge>
              <tokens-count>
                <place>P2</place>
              </tokens-count>
              <integer-constant>2</integer-constant>
            </integer-ge>
            <integer-ge>
              <tokens-count>
                <place>P4</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-ge>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
