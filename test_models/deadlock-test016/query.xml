<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>deadlock-test016.TRUE-1</id>
    <description>deadlock-test016.TRUE-1</description>
    <formula>
      <exists-path>
        <globally>
          <negation>
            <deadlock/>
          </negation>
        </globally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>deadlock-test016.FALSE-2</id>
    <description>deadlock-test016.FALSE-2</description>
    <formula>
      <all-paths>
        <globally>
          <negation>
            <conjunction>
              <deadlock/>
              <integer-le>
                <tokens-count>
                  <place>P0</place>
                </tokens-count>
                <integer-constant>2</integer-constant>
              </integer-le>
            </conjunction>
          </negation>
        </globally>
      </all-paths>
    </formula>
  </property>
</property-set>
