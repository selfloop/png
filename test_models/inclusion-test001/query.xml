<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>inclusion-test001.FALSE-1</id>
    <description>inclusion-test001.FALSE-1</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-ge>
              <tokens-count>
                <place>all</place>
              </tokens-count>
              <integer-constant>2</integer-constant>
            </integer-ge>
            <integer-eq>
              <tokens-count>
                <place>minus</place>
              </tokens-count>
              <integer-constant>3</integer-constant>
            </integer-eq>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
