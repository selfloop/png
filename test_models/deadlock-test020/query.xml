<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>deadlock-test020.TRUE-1</id>
    <description>deadlock-test020.TRUE-1</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-eq>
              <tokens-count>
                <place>P3</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-eq>
            <negation>
              <deadlock/>
            </negation>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
