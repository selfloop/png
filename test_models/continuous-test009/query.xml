<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>continuous-test009.FALSE-1</id>
    <description>continuous-test009.FALSE-1</description>
    <formula>
      <all-paths>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>out</place>
            </tokens-count>
            <integer-constant>1</integer-constant>
          </integer-eq>
        </finally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>continuous-test009.TRUE-2</id>
    <description>continuous-test009.TRUE-2</description>
    <formula>
      <exists-path>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>out</place>
            </tokens-count>
            <integer-constant>1</integer-constant>
          </integer-eq>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>continuous-test009.TRUE-3</id>
    <description>continuous-test009.TRUE-3</description>
    <formula>
      <exists-path>
        <finally>
          <deadlock/>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>continuous-test009.FALSE-4</id>
    <description>continuous-test009.FALSE-4</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <deadlock/>
            <integer-eq>
              <tokens-count>
                <place>out</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
</property-set>
