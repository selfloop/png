              </integer-product>
              <tokens-count>
                <place>P1</place>
              </tokens-count>
              <tokens-count>
                <place>P2</place>
              </tokens-count>
            </integer-difference>
            <integer-constant>0</integer-constant>
          </integer-ne>
        </finally>
      </exists-path>
    </formula>
  </property>
