<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>arithmetic-test006.TRUE-1</id>
    <description>arithmetic-test006.TRUE-1</description>
    <formula>
      <exists-path>
        <finally>
          <integer-eq>
            <integer-difference>
              <integer-product>
                <integer-constant>2</integer-constant>
                <tokens-count>
                  <place>P0</place>
                </tokens-count>
              </integer-product>
              <tokens-count>
                <place>P1</place>
              </tokens-count>
              <tokens-count>
                <place>P2</place>
              </tokens-count>
            </integer-difference>
            <integer-constant>0</integer-constant>
          </integer-eq>
        </finally>
      </exists-path>
    </formula>
  </property>
