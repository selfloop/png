<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>reduction-008-house.FALSE-1</id>
    <description>reduction-008-house.FALSE-1</description>
    <formula>
      <exists-path>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>p15</place>
            </tokens-count>
            <integer-constant>3</integer-constant>
          </integer-eq>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>reduction-008-house.TRUE-2</id>
    <description>reduction-008-house.TRUE-2</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-eq>
              <tokens-count>
                <place>p26</place>
              </tokens-count>
              <integer-constant>2</integer-constant>
            </integer-eq>
            <integer-ge>
              <tokens-count>
                <place>p7</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-ge>
          </conjunction>
        </finally>
