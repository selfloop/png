<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>inclusion-test004.TRUE-1</id>
    <description>inclusion-test004.TRUE-1</description>
    <formula>
      <exists-path>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>end</place>
            </tokens-count>
            <integer-constant>5</integer-constant>
          </integer-eq>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>inclusion-test004.FALSE-2</id>
    <description>inclusion-test004.FALSE-2</description>
    <formula>
      <all-paths>
        <globally>
          <disjunction>
            <integer-ge>
              <tokens-count>
                <place>P0</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-ge>
            <integer-le>
              <tokens-count>
                <place>end</place>
              </tokens-count>
              <integer-constant>4</integer-constant>
            </integer-le>
          </disjunction>
        </globally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>inclusion-test004.TRUE-3</id>
    <description>inclusion-test004.TRUE-3</description>
    <formula>
      <exists-path>
        <finally>
          <disjunction>
            <integer-gt>
              <tokens-count>
                <place>end</place>
              </tokens-count>
              <integer-constant>5</integer-constant>
            </integer-gt>
            <conjunction>
              <integer-lt>
                <tokens-count>
                  <place>P2</place>
                </tokens-count>
                <integer-constant>2</integer-constant>
              </integer-lt>
              <integer-gt>
                <tokens-count>
                  <place>P2</place>
                </tokens-count>
                <integer-constant>0</integer-constant>
              </integer-gt>
            </conjunction>
          </disjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>inclusion-test004.FALSE-4</id>
    <description>inclusion-test004.FALSE-4</description>
    <formula>
      <exists-path>
        <finally>
          <disjunction>
            <integer-gt>
              <tokens-count>
                <place>end</place>
              </tokens-count>
              <integer-constant>5</integer-constant>
            </integer-gt>
            <conjunction>
              <integer-lt>
                <tokens-count>
                  <place>P2</place>
                </tokens-count>
                <integer-constant>4</integer-constant>
              </integer-lt>
              <integer-eq>
                <tokens-count>
                  <place>P1</place>
                </tokens-count>
                <integer-constant>4</integer-constant>
              </integer-eq>
              <integer-eq>
                <tokens-count>
                  <place>P4</place>
                </tokens-count>
                <integer-constant>2</integer-constant>
              </integer-eq>
              <integer-eq>
                <tokens-count>
                  <place>P5</place>
                </tokens-count>
                <integer-constant>1</integer-constant>
              </integer-eq>
            </conjunction>
          </disjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
</property-set>
