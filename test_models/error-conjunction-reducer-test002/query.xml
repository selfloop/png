<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>error-conjunction-reducer-test002.TRUE-1</id>
    <description>error-conjunction-reducer-test002.TRUE-1</description>
    <formula>
      <exists-path>
        <finally>
          <negation>
            <disjunction>
              <is-fireable>
                <transition>t1</transition>
              </is-fireable>
              <conjunction>
                <negation>
                  <is-fireable>
                    <transition>t18</transition>
                  </is-fireable>
                </negation>
                <is-fireable>
                  <transition>t18</transition>
                </is-fireable>
              </conjunction>
            </disjunction>
          </negation>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>error-conjunction-reducer-test002.FALSE-2</id>
    <description>error-conjunction-reducer-test002.FALSE-2</description>
    <formula>
      <all-paths>
        <globally>
          <disjunction>
            <is-fireable>
              <transition>t4</transition>
            </is-fireable>
            <disjunction>
              <negation>
                <negation>
                  <is-fireable>
                    <transition>t1</transition>
                  </is-fireable>
                </negation>
              </negation>
              <negation>
                <negation>
                  <is-fireable>
                    <transition>t5</transition>
                  </is-fireable>
                </negation>
              </negation>
            </disjunction>
          </disjunction>
        </globally>
      </all-paths>
    </formula>
  </property>
</property-set>
