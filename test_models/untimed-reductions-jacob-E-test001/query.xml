<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>untimed-reductions-jacob-E-test001.TRUE-1</id>
    <description>untimed-reductions-jacob-E-test001.TRUE-1</description>
    <formula>
      <exists-path>
        <finally>
          <integer-gt>
            <tokens-count>
              <place>P8</place>
            </tokens-count>
            <integer-constant>2</integer-constant>
          </integer-gt>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>untimed-reductions-jacob-E-test001.TRUE-2</id>
    <description>untimed-reductions-jacob-E-test001.TRUE-2</description>
    <formula>
      <all-paths>
        <globally>
          <integer-eq>
            <tokens-count>
              <place>P2</place>
            </tokens-count>
            <integer-constant>2</integer-constant>
          </integer-eq>
        </globally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>untimed-reductions-jacob-E-test001.TRUE-3</id>
    <description>untimed-reductions-jacob-E-test001.TRUE-3</description>
    <formula>
      <all-paths>
        <globally>
          <integer-gt>
            <tokens-count>
              <place>P0</place>
            </tokens-count>
            <integer-constant>0</integer-constant>
          </integer-gt>
        </globally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>untimed-reductions-jacob-E-test001.TRUE-4</id>
    <description>untimed-reductions-jacob-E-test001.TRUE-4</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-eq>
              <tokens-count>
                <place>P6</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>P8</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-eq>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
</property-set>
