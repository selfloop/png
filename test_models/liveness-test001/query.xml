<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>liveness-test001.TRUE-1</id>
    <description>liveness-test001.TRUE-1</description>
    <formula>
      <all-paths>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>P4</place>
            </tokens-count>
            <integer-constant>0</integer-constant>
          </integer-eq>
        </finally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>liveness-test001.TRUE-2</id>
    <description>liveness-test001.TRUE-2</description>
    <formula>
      <exists-path>
        <globally>
          <integer-eq>
            <tokens-count>
              <place>P4</place>
            </tokens-count>
            <integer-constant>0</integer-constant>
          </integer-eq>
        </globally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>liveness-test001.FALSE-3</id>
    <description>liveness-test001.FALSE-3</description>
    <formula>
      <exists-path>
        <globally>
          <conjunction>
            <integer-lt>
              <tokens-count>
                <place>P3</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-lt>
            <integer-eq>
              <tokens-count>
                <place>P4</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
          </conjunction>
        </globally>
      </exists-path>
    </formula>
  </property>
</property-set>
