<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>urgency-test009.TRUE-1</id>
    <description>urgency-test009.TRUE-1</description>
    <formula>
      <exists-path>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>final</place>
            </tokens-count>
            <integer-constant>2</integer-constant>
          </integer-eq>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>urgency-test009.FALSE-2</id>
    <description>urgency-test009.FALSE-2</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-eq>
              <tokens-count>
                <place>final</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>P5</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-eq>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>urgency-test009.FALSE-3</id>
    <description>urgency-test009.FALSE-3</description>
    <formula>
      <all-paths>
        <globally>
          <integer-lt>
            <tokens-count>
              <place>final</place>
            </tokens-count>
            <integer-constant>2</integer-constant>
          </integer-lt>
        </globally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>urgency-test009.FALSE-4</id>
    <description>urgency-test009.FALSE-4</description>
    <formula>
      <all-paths>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>final</place>
            </tokens-count>
            <integer-constant>2</integer-constant>
          </integer-eq>
        </finally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>urgency-test009.TRUE-5</id>
    <description>urgency-test009.TRUE-5</description>
    <formula>
      <exists-path>
        <globally>
          <integer-gt>
            <tokens-count>
              <place>P5</place>
            </tokens-count>
            <integer-constant>0</integer-constant>
          </integer-gt>
        </globally>
      </exists-path>
    </formula>
  </property>
  <property>
