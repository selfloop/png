<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>Query Comment/Name Here</id>
    <description>Query Comment/Name Here</description>
    <formula>
      <all-paths>
        <until>
          <before>
            <integer-eq>
              <tokens-count>
                <place>P0</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-eq>
          </before>
          <reach>
            <integer-eq>
              <tokens-count>
                <place>P1</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-eq>
          </reach>
        </until>
      </all-paths>
    </formula>
  </property>
</property-set>
