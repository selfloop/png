<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>discrete-test004.TRUE-1</id>
    <description>discrete-test004.TRUE-1</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-eq>
              <tokens-count>
                <place>P6</place>
              </tokens-count>
              <integer-constant>2</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>P5</place>
              </tokens-count>
              <integer-constant>2</integer-constant>
            </integer-eq>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>discrete-test004.TRUE-2</id>
    <description>discrete-test004.TRUE-2</description>
    <formula>
      <all-paths>
        <globally>
          <negation>
            <conjunction>
              <integer-ge>
                <tokens-count>
                  <place>P6</place>
                </tokens-count>
                <integer-constant>3</integer-constant>
              </integer-ge>
              <integer-ge>
                <tokens-count>
                  <place>P5</place>
                </tokens-count>
                <integer-constant>2</integer-constant>
              </integer-ge>
            </conjunction>
          </negation>
        </globally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>discrete-test004.FALSE-3</id>
    <description>discrete-test004.FALSE-3</description>
    <formula>
      <all-paths>
        <globally>
          <negation>
            <conjunction>
              <integer-ge>
                <tokens-count>
                  <place>P6</place>
                </tokens-count>
                <integer-constant>2</integer-constant>
              </integer-ge>
              <integer-ge>
                <tokens-count>
                  <place>P5</place>
                </tokens-count>
                <integer-constant>2</integer-constant>
              </integer-ge>
            </conjunction>
          </negation>
        </globally>
      </all-paths>
    </formula>
  </property>
</property-set>
