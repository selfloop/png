<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>workflow-test014.TRUE-1</id>
    <description>workflow-test014.TRUE-1</description>
    <formula>
      <exists-path>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>Shared_out</place>
            </tokens-count>
            <integer-constant>1</integer-constant>
          </integer-eq>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>workflow-test014.FALSE-2</id>
    <description>workflow-test014.FALSE-2</description>
    <formula>
      <exists-path>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>Monitor_finished_in_time</place>
            </tokens-count>
            <integer-constant>1</integer-constant>
          </integer-eq>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>workflow-test014.TRUE-3</id>
    <description>workflow-test014.TRUE-3</description>
    <formula>
      <all-paths>
        <globally>
          <disjunction>
            <integer-eq>
              <tokens-count>
                <place>Shared_out</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
            <conjunction>
              <integer-eq>
                <tokens-count>
                  <place>Shared_in</place>
                </tokens-count>
                <integer-constant>0</integer-constant>
              </integer-eq>
              <integer-eq>
                <tokens-count>
                  <place>Registration_P1</place>
                </tokens-count>
                <integer-constant>0</integer-constant>
              </integer-eq>
              <integer-eq>
                <tokens-count>
                  <place>Registration_P2</place>
                </tokens-count>
                <integer-constant>0</integer-constant>
              </integer-eq>
              <integer-eq>
                <tokens-count>
                  <place>Registration_P3</place>
                </tokens-count>
                <integer-constant>0</integer-constant>
              </integer-eq>
              <integer-eq>
                <tokens-count>
                  <place>Registration_P4</place>
                </tokens-count>
                <integer-constant>0</integer-constant>
              </integer-eq>
              <integer-eq>
                <tokens-count>
                  <place>Handling_P0</place>
                </tokens-count>
                <integer-constant>0</integer-constant>
              </integer-eq>
              <integer-eq>
                <tokens-count>
                  <place>Handling_P1</place>
                </tokens-count>
                <integer-constant>0</integer-constant>
              </integer-eq>
              <integer-eq>
                <tokens-count>
                  <place>Handling_P2</place>
                </tokens-count>
                <integer-constant>0</integer-constant>
              </integer-eq>
              <integer-eq>
                <tokens-count>
                  <place>Handling_P3</place>
                </tokens-count>
                <integer-constant>0</integer-constant>
              </integer-eq>
              <integer-eq>
                <tokens-count>
                  <place>Monitor_finished_in_time</place>
                </tokens-count>
                <integer-constant>0</integer-constant>
              </integer-eq>
            </conjunction>
          </disjunction>
        </globally>
      </all-paths>
    </formula>
  </property>
</property-set>
