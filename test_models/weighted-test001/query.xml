<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>weighted-test001.TRUE-1</id>
    <description>weighted-test001.TRUE-1</description>
    <formula>
      <exists-path>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>P6</place>
            </tokens-count>
            <integer-constant>1</integer-constant>
          </integer-eq>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>weighted-test001.TRUE-2</id>
    <description>weighted-test001.TRUE-2</description>
    <formula>
      <exists-path>
        <globally>
          <integer-ge>
            <tokens-count>
              <place>P2</place>
            </tokens-count>
            <integer-constant>1</integer-constant>
          </integer-ge>
        </globally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>weighted-test001.FALSE-3</id>
    <description>weighted-test001.FALSE-3</description>
    <formula>
      <all-paths>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>P6</place>
            </tokens-count>
            <integer-constant>2</integer-constant>
          </integer-eq>
        </finally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>weighted-test001.FALSE-4</id>
    <description>weighted-test001.FALSE-4</description>
    <formula>
      <all-paths>
        <globally>
          <integer-lt>
            <tokens-count>
              <place>P6</place>
            </tokens-count>
            <integer-constant>2</integer-constant>
          </integer-lt>
        </globally>
      </all-paths>
    </formula>
  </property>
</property-set>
