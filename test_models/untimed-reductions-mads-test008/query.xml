<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>untimed-reductions-mads-test008.FALSE-1</id>
    <description>untimed-reductions-mads-test008.FALSE-1</description>
    <formula>
      <exists-path>
        <finally>
          <integer-ge>
            <tokens-count>
              <place>P2</place>
            </tokens-count>
            <integer-constant>3</integer-constant>
          </integer-ge>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>untimed-reductions-mads-test008.TRUE-2</id>
    <description>untimed-reductions-mads-test008.TRUE-2</description>
    <formula>
      <exists-path>
        <finally>
          <deadlock/>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>untimed-reductions-mads-test008.TRUE-3</id>
    <description>untimed-reductions-mads-test008.TRUE-3</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-ge>
              <tokens-count>
                <place>P0</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-ge>
            <true/>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>untimed-reductions-mads-test008.TRUE-4</id>
    <description>untimed-reductions-mads-test008.TRUE-4</description>
    <formula>
      <all-paths>
        <globally>
          <integer-eq>
            <tokens-count>
              <place>P2</place>
            </tokens-count>
            <integer-constant>0</integer-constant>
          </integer-eq>
        </globally>
      </all-paths>
    </formula>
  </property>
</property-set>
