<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>inclusion-test006.TRUE-1</id>
    <description>inclusion-test006.TRUE-1</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-le>
              <tokens-count>
                <place>P2</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-le>
            <integer-eq>
              <tokens-count>
                <place>P1</place>
              </tokens-count>
              <integer-constant>4</integer-constant>
            </integer-eq>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>inclusion-test006.TRUE-2</id>
    <description>inclusion-test006.TRUE-2</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-eq>
              <tokens-count>
                <place>P2</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>P1</place>
              </tokens-count>
              <integer-constant>4</integer-constant>
            </integer-eq>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
</property-set>
