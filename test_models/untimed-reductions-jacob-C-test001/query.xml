<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>untimed-reductions-jacob-C-test001.TRUE-1</id>
    <description>untimed-reductions-jacob-C-test001.TRUE-1</description>
    <formula>
      <exists-path>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>inhib1</place>
            </tokens-count>
            <integer-constant>2</integer-constant>
          </integer-eq>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>untimed-reductions-jacob-C-test001.FALSE-2</id>
    <description>untimed-reductions-jacob-C-test001.FALSE-2</description>
    <formula>
      <all-paths>
        <globally>
          <integer-eq>
            <tokens-count>
              <place>inhib0</place>
            </tokens-count>
            <integer-constant>0</integer-constant>
          </integer-eq>
        </globally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>untimed-reductions-jacob-C-test001.FALSE-3</id>
    <description>untimed-reductions-jacob-C-test001.FALSE-3</description>
    <formula>
      <all-paths>
        <globally>
          <disjunction>
            <integer-eq>
              <tokens-count>
                <place>inhib0</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>inhib1</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
          </disjunction>
        </globally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>untimed-reductions-jacob-C-test001.FALSE-4</id>
    <description>untimed-reductions-jacob-C-test001.FALSE-4</description>
    <formula>
      <all-paths>
        <globally>
          <integer-eq>
            <tokens-count>
              <place>T0count</place>
            </tokens-count>
            <integer-constant>0</integer-constant>
          </integer-eq>
        </globally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>untimed-reductions-jacob-C-test001.FALSE-5</id>
    <description>untimed-reductions-jacob-C-test001.FALSE-5</description>
    <formula>
      <all-paths>
        <globally>
          <integer-eq>
            <tokens-count>
              <place>T1count</place>
            </tokens-count>
            <integer-constant>0</integer-constant>
          </integer-eq>
        </globally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>untimed-reductions-jacob-C-test001.TRUE-6</id>
    <description>untimed-reductions-jacob-C-test001.TRUE-6</description>
    <formula>
      <exists-path>
        <finally>
          <disjunction>
            <conjunction>
              <integer-gt>
                <tokens-count>
                  <place>P0</place>
                </tokens-count>
                <integer-constant>0</integer-constant>
              </integer-gt>
              <integer-eq>
                <tokens-count>
                  <place>P1</place>
                </tokens-count>
                <integer-constant>0</integer-constant>
              </integer-eq>
            </conjunction>
            <integer-ge>
              <tokens-count>
                <place>T0count</place>
              </tokens-count>
              <integer-constant>10</integer-constant>
            </integer-ge>
          </disjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
</property-set>
