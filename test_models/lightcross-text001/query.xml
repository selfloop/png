<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>lightcross-text001.FALSE-1</id>
    <description>lightcross-text001.FALSE-1</description>
    <formula>
      <all-paths>
        <globally>
          <negation>
            <conjunction>
              <integer-ge>
                <tokens-count>
                  <place>R1</place>
                </tokens-count>
                <integer-constant>1</integer-constant>
              </integer-ge>
              <integer-ge>
                <tokens-count>
                  <place>R2</place>
                </tokens-count>
                <integer-constant>1</integer-constant>
              </integer-ge>
            </conjunction>
          </negation>
        </globally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>lightcross-text001.FALSE-2</id>
    <description>lightcross-text001.FALSE-2</description>
    <formula>
      <all-paths>
        <globally>
          <negation>
            <negation>
              <conjunction>
                <integer-eq>
                  <tokens-count>
                    <place>G1</place>
                  </tokens-count>
                  <integer-constant>1</integer-constant>
                </integer-eq>
                <integer-eq>
                  <tokens-count>
                    <place>G2</place>
                  </tokens-count>
                  <integer-constant>1</integer-constant>
                </integer-eq>
              </conjunction>
            </negation>
          </negation>
        </globally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>lightcross-text001.TRUE-3</id>
    <description>lightcross-text001.TRUE-3</description>
    <formula>
      <all-paths>
        <globally>
          <negation>
            <conjunction>
              <integer-ge>
                <tokens-count>
                  <place>GU1</place>
                </tokens-count>
                <integer-constant>1</integer-constant>
              </integer-ge>
              <integer-ge>
                <tokens-count>
                  <place>GU2</place>
                </tokens-count>
                <integer-constant>1</integer-constant>
              </integer-ge>
            </conjunction>
          </negation>
        </globally>
      </all-paths>
    </formula>
  </property>
</property-set>
