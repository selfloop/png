<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>urgency-test013.FALSE-1</id>
    <description>urgency-test013.FALSE-1</description>
    <formula>
      <all-paths>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>out</place>
            </tokens-count>
            <integer-constant>1</integer-constant>
          </integer-eq>
        </finally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>urgency-test013.TRUE-2</id>
    <description>urgency-test013.TRUE-2</description>
    <formula>
      <all-paths>
        <globally>
          <disjunction>
            <negation>
              <integer-ge>
                <tokens-count>
                  <place>out</place>
                </tokens-count>
                <integer-constant>1</integer-constant>
              </integer-ge>
            </negation>
            <conjunction>
              <integer-eq>
                <tokens-count>
                  <place>out</place>
                </tokens-count>
                <integer-constant>1</integer-constant>
              </integer-eq>
              <integer-eq>
                <tokens-count>
                  <place>in</place>
                </tokens-count>
                <integer-constant>0</integer-constant>
              </integer-eq>
              <integer-eq>
                <tokens-count>
                  <place>booking</place>
                </tokens-count>
                <integer-constant>0</integer-constant>
              </integer-eq>
              <integer-eq>
                <tokens-count>
                  <place>payment</place>
                </tokens-count>
                <integer-constant>0</integer-constant>
              </integer-eq>
              <integer-eq>
                <tokens-count>
                  <place>attempts</place>
                </tokens-count>
                <integer-constant>0</integer-constant>
              </integer-eq>
            </conjunction>
          </disjunction>
        </globally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>urgency-test013.TRUE-3</id>
    <description>urgency-test013.TRUE-3</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-eq>
              <tokens-count>
                <place>succesful</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>succesful</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-eq>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>urgency-test013.FALSE-4</id>
    <description>urgency-test013.FALSE-4</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-eq>
              <tokens-count>
                <place>attempts</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>out</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-eq>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
</property-set>
