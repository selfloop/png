<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>inclusion-test007.FALSE-1</id>
    <description>inclusion-test007.FALSE-1</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-eq>
              <tokens-count>
                <place>P2</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>P1</place>
              </tokens-count>
              <integer-constant>4</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>P3</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>inclusion-test007.FALSE-2</id>
    <description>inclusion-test007.FALSE-2</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-le>
              <tokens-count>
                <place>P2</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-le>
            <integer-ge>
              <tokens-count>
                <place>P1</place>
              </tokens-count>
              <integer-constant>4</integer-constant>
            </integer-ge>
            <integer-lt>
              <tokens-count>
                <place>P3</place>
              </tokens-count>
              <integer-constant>2</integer-constant>
            </integer-lt>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>inclusion-test007.TRUE-3</id>
    <description>inclusion-test007.TRUE-3</description>
    <formula>
      <all-paths>
        <globally>
          <disjunction>
            <integer-gt>
              <tokens-count>
                <place>P1</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-gt>
            <conjunction>
              <integer-gt>
                <tokens-count>
                  <place>P0</place>
                </tokens-count>
                <integer-constant>1</integer-constant>
              </integer-gt>
              <disjunction>
                <integer-eq>
                  <tokens-count>
                    <place>P2</place>
                  </tokens-count>
                  <integer-constant>0</integer-constant>
                </integer-eq>
                <integer-eq>
                  <tokens-count>
                    <place>P3</place>
                  </tokens-count>
                  <integer-constant>0</integer-constant>
                </integer-eq>
              </disjunction>
            </conjunction>
          </disjunction>
        </globally>
      </all-paths>
    </formula>
  </property>
</property-set>
