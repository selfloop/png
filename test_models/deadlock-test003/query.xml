<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>deadlock-test003.TRUE-1</id>
    <description>deadlock-test003.TRUE-1</description>
    <formula>
      <all-paths>
        <finally>
          <conjunction>
            <integer-eq>
              <tokens-count>
                <place>WorkflowNet1_out</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>WorkflowNet2_out</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-eq>
          </conjunction>
        </finally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>deadlock-test003.TRUE-2</id>
    <description>deadlock-test003.TRUE-2</description>
    <formula>
      <exists-path>
        <finally>
          <deadlock/>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>deadlock-test003.FALSE-3</id>
    <description>deadlock-test003.FALSE-3</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <deadlock/>
            <disjunction>
              <integer-lt>
                <tokens-count>
                  <place>WorkflowNet1_out</place>
                </tokens-count>
                <integer-constant>0</integer-constant>
              </integer-lt>
              <integer-lt>
                <tokens-count>
                  <place>WorkflowNet2_out</place>
                </tokens-count>
                <integer-constant>1</integer-constant>
              </integer-lt>
            </disjunction>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
</property-set>
