<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>urgency-test005.TRUE-1</id>
    <description>urgency-test005.TRUE-1</description>
    <formula>
      <exists-path>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>P11</place>
            </tokens-count>
            <integer-constant>2</integer-constant>
          </integer-eq>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>urgency-test005.FALSE-2</id>
    <description>urgency-test005.FALSE-2</description>
    <formula>
      <all-paths>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>P11</place>
            </tokens-count>
            <integer-constant>1</integer-constant>
          </integer-eq>
        </finally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>urgency-test005.TRUE-3</id>
    <description>urgency-test005.TRUE-3</description>
    <formula>
      <exists-path>
        <globally>
          <integer-eq>
            <tokens-count>
              <place>P11</place>
            </tokens-count>
            <integer-constant>0</integer-constant>
          </integer-eq>
        </globally>
      </exists-path>
    </formula>
  </property>
</property-set>
