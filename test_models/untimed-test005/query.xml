<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>untimed-test005.TRUE-1</id>
    <description>untimed-test005.TRUE-1</description>
    <formula>
      <all-paths>
        <finally>
          <deadlock/>
        </finally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>untimed-test005.TRUE-2</id>
    <description>untimed-test005.TRUE-2</description>
    <formula>
      <all-paths>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>done</place>
            </tokens-count>
            <integer-constant>1</integer-constant>
          </integer-eq>
        </finally>
      </all-paths>
    </formula>
  </property>
  <property>
    <id>untimed-test005.TRUE-3</id>
    <description>untimed-test005.TRUE-3</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-eq>
              <tokens-count>
                <place>r1</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>r2</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>r4</place>
              </tokens-count>
              <integer-constant>1</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>r8</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>untimed-test005.TRUE-4</id>
    <description>untimed-test005.TRUE-4</description>
    <formula>
      <exists-path>
        <globally>
          <conjunction>
            <integer-eq>
              <tokens-count>
                <place>r1</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>r4</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
          </conjunction>
        </globally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>untimed-test005.FALSE-5</id>
    <description>untimed-test005.FALSE-5</description>
    <formula>
      <exists-path>
        <globally>
          <conjunction>
            <integer-eq>
              <tokens-count>
                <place>r1</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>r4</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>done</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-eq>
          </conjunction>
        </globally>
      </exists-path>
    </formula>
  </property>
</property-set>
