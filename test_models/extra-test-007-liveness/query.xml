<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>extra-test-007-liveness.TRUE-1</id>
    <description>extra-test-007-liveness.TRUE-1</description>
    <formula>
      <exists-path>
        <globally>
          <negation>
            <integer-gt>
              <tokens-count>
                <place>TAPN2_Error</place>
              </tokens-count>
              <integer-constant>0</integer-constant>
            </integer-gt>
          </negation>
        </globally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>extra-test-007-liveness.TRUE-2</id>
    <description>extra-test-007-liveness.TRUE-2</description>
    <formula>
      <exists-path>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>TAPN2_Error</place>
            </tokens-count>
            <integer-constant>2</integer-constant>
          </integer-eq>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>extra-test-007-liveness.FALSE-3</id>
    <description>extra-test-007-liveness.FALSE-3</description>
    <formula>
      <exists-path>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>TAPN2_Error</place>
            </tokens-count>
            <integer-constant>3</integer-constant>
          </integer-eq>
        </finally>
      </exists-path>
    </formula>
  </property>
  <property>
    <id>extra-test-007-liveness.FALSE-4</id>
    <description>extra-test-007-liveness.FALSE-4</description>
    <formula>
      <all-paths>
        <finally>
          <integer-eq>
            <tokens-count>
              <place>Shared_P4s</place>
            </tokens-count>
            <integer-constant>1</integer-constant>
          </integer-eq>
        </finally>
      </all-paths>
    </formula>
  </property>
</property-set>
